label_resp <- function(direction, response){
cnt <- 1
for (resp in response){
if (resp == 'v' & direction[cnt] == '-' | resp == 'n' & direction[cnt] == '+'){
label = 1
}
if (resp == 'n' & direction[cnt] == '-' | resp == 'v' & direction[cnt] == '+'){
label <- 0
}
else {
label <- NA
}
label_vec[cnt] <- label
cnt <- cnt+1
}
return(label_vec)
}
