get_nd_time <- function(data, colname){
# get mean and sd from data
dat_mean <- mean(data$colname)
dat_sd   <- sd(data$colname)
# remove data larger than 3 sd
reaction_values <- data$colname[-which(data$colname<(dat_mean - 3*dat_sd))]
return(min(reaction_values))
}
